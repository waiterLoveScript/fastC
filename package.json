{
  "name": "fastc",
  "displayName": "fastC",
  "description": "无需任何操作, 一键配置C/C++环境",
  "publisher": "LoveScript",
  "icon": "icons/icon1.jpg",
  "repository": {
    "type": "git",
    "url": "https://github.com/waiterLoveScript/fastC.git"
  },
  "version": "1.2.0",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand: fastc.install"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fastc.install",
        "title": "一键配置C/C++"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.5.1"
  }
}
